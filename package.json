{
  "name": "s-comp",
  "version": "0.1.0",
  "description": "Some Svelte Components",
  "svelte": "src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "clean": "rimraf build/*",
    "bundle-monaco-editor": "cd vendor/monaco-editor/browser-rollup-custom && sh bundle.sh dist",
    "bundle-abcjs": "cd vendor/abcjs && rollup -c",
    "bundle-mathjax": "cd vendor/mathjax && sh bundle.sh dist",
    "predev": "rimraf build/dev",
    "dev": "rollup -c -w",
    "predev4096": "rimraf build/dev",
    "dev4096": "node --max-old-space-size=4096 node_modules/rollup/dist/bin/rollup -c -w",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "rimraf build/dist",
    "build": "rollup -c",
    "prebuild-pages-dist": "rimraf build/dist",
    "build-pages-dist": "rollup -c",
    "precopy-build-to-extension": "rimraf extension/chrome/build extension/chrome/test extension/chrome/vendor",
    "copy-build-to-extension": "cp -R build extension/chrome && cp -R test extension/chrome && cp -R vendor extension/chrome"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ghjang/s-comp.git"
  },
  "keywords": [
    "svelte",
    "components",
    "ui",
    "abc-notation",
    "pdf-viewer",
    "monaco-editor"
  ],
  "author": "ghjang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ghjang/s-comp/issues"
  },
  "homepage": "https://ghjang.github.io/s-comp/",
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@rollup/plugin-url": "^8.0.2",
    "@types/lodash-es": "^4.17.12",
    "css-loader": "^7.1.2",
    "file-loader": "^6.2.0",
    "glob": "^11.0.0",
    "rimraf": "^5.0.5",
    "rollup": "^2.79.1",
    "rollup-plugin-css-only": "^4.5.2",
    "rollup-plugin-execute": "^1.1.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-styles": "^4.0.0",
    "rollup-plugin-svelte": "^7.2.0",
    "style-loader": "^4.0.0",
    "svelte": "^4.2.19",
    "svelte-preprocess": "^5.1.4",
    "terser-webpack-plugin": "^5.3.10",
    "tslib": "^2.7.0",
    "typescript": "^5.5.4",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "abcjs": "^6.3.0",
    "autoprefixer": "^10.4.19",
    "js-yaml": "^4.1.0",
    "jspdf": "^2.5.1",
    "lodash-es": "^4.17.21",
    "mathjax": "^3.2.2",
    "mathjax-full": "^3.2.2",
    "monaco-editor": "^0.47.0",
    "pdfjs-dist": "^4.3.136",
    "pyodide": "^0.26.1",
    "s-comp-core": "file:../s-comp-core/s-comp-core-0.0.1.tgz",
    "sass": "^1.76.0",
    "svg2pdf.js": "^2.2.4"
  },
  "peerDependencies": {
    "mathjax": "^3.2.2",
    "monaco-editor": "^0.47.0",
    "pdfjs-dist": "^4.3.136",
    "pyodide": "^0.26.1",
    "svelte": "^4.2.19"
  }
}
